: sgi.mips2 cc wrapper that generates mips2 binaries : 1999-08-11 :

HOSTTYPE=sgi.mips2

case $* in
-dumpmachine) echo $HOSTTYPE; exit ;;
esac

if	test -d /usr/lib32
then	LD_LIBRARYN32_PATH=/lib32 /usr/bin/cc -32 -mips2 -G0 -Olimit 5000 -Wl,-woff,56 -Wl,-woff,15 -Wl,-woff,84 -Wl,-woff,85 -Wl,-woff,134 -woff 1685,733,1048,1155,1171,1184,1209,1343,3169,3170,3433 "$@"
else	/usr/bin/cc -mips2 -Olimit 5000 -woff 1685,733,1048,1155,1171,1184,1209,1343,3169,3170,3433 "$@"
fi
